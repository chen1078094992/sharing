{"appenders":
  [
    {
      "type":"console",
      "category":"console"
    },
    {
      "category":"log_file",
      "type": "file",
      "filename": "../social-auth/common/logs/log_file/file.log",
      "maxLogSize": 1048000,
      "backups": 100
    },
    {
      "category":"log_error",
      "type": "file",
      "filename": "../social-auth/common/logs/log_error/error.log",
      "maxLogSize": 104800,
      "backups": 100
    },
    {
      "category":"log_date",
      "type": "dateFile",
      "filename": "../social-auth/common/logs/log_date/date",
      "alwaysIncludePattern": true,
      "pattern": "-yyyy-MM-dd-hh.log"

    }
  ],
  "replaceConsole": true,
  "levels":
  {
    "log_file":"ALL",
    "console":"ALL",
    "log_date":"ALL"
  }
}
